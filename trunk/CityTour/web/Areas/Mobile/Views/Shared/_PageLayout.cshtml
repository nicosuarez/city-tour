@if (IsSectionDefined("pageId"))
{
@:<div id="@Html.AttributeEncode(RenderSection("pageId", false)).Trim()" data-role="page">
}
else 
{ 
@:<div data-role="page">
}
    <div data-role="header" data-theme="b">
        @RenderSection("header", false)
	</div>
    <div data-role="content">
        @RenderBody()
	</div>
    <div data-role="footer" data-theme="b">
        @RenderSection("footer", false)
    </div> 
</div>
